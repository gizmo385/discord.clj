{:paths ["src"]
 :deps {org.clojure/clojure {:mvn/version "1.10.0"}
		org.clojure/core.async {:mvn/version "0.3.443"}
		clj-http/clj-http {:mvn/version "3.6.1"}
		clj-time/clj-time {:mvn/version "0.14.4"}
		com.taoensso/timbre {:mvn/version "4.10.0"}
		org.clojure/data.json {:mvn/version "0.2.6"}
		overtone/at-at {:mvn/version "1.2.0"}
		stylefruits/gniazdo {:mvn/version "1.0.0"}
        shutdown/shutdown  {:mvn/version "0.1.0-SNAPSHOT"}}
 :aliases {:uberjar {:replace-deps  {com.github.seancorfield/depstar  {:mvn/version "2.0.193"}}
                     :exec-fn hf.depstar/uberjar
                     :exec-args  {:jar "target/bot.jar"
                                  :aot true
                                  :main-class discord.core}}
           :run {:main-opts ["-m" "discord.core"]}
           :nrepl {:extra-deps  {nrepl/nrepl  {:mvn/version "RELEASE"}
                                 cider/cider-nrepl {:mvn/version "0.22.4"}
                                 cider/piggieback {:mvn/version "0.3.8"}}
                   :main-opts  ["-m" "nrepl.cmdline" "--interactive"]
                   :extra-paths ["test"]}}}
